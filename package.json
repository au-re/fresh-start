  "private": true,
  "dependencies": {
    "antd": "^3.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "styled-components": "^2.3.0"
  },
  "scripts": {
